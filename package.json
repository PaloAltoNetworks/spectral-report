{
  "name": "@paloaltonetworks/spectral-report",
  "version": "2.0.0-alpha0",
  "description": "Generates report for the spectral linter output",
  "main": "./lib/index.js",
  "bin": {
    "spectral-report": "./lib/index.js"
  },
  "preferGlobal": true,
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
